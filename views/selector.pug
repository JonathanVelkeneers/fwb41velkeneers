extends layout

block content
    h1= title

    - var imageNames=["Jblock.png","Oblock.png","Sblock.jpeg","Tblock.png","Zblock.png"]

    - for(let i = 0 ; i < 3 ; i++){
        - var random = Math.floor(Math.random()*5)
        img(src=`/images/${imageNames[random]}`, alt="Tetris block", width=300, height=300)
    - }